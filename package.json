{
  "name": "node-api-starter",
  "version": "1.0.2",
  "description": "Full-featured starter for Node Rest API server using ExpressJS",
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/node-api-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/node-api-starter/issues"
  },
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.15.2"
  },
  "main": "src/index.js",
  "scripts": {
    "format": "prettier --write '**/*.{js,json,md}'",
    "lint": "node ./node_modules/eslint/bin/eslint.js '**/*.js'",
    "seed": "node src/seeds/index.js",
    "start": "cross-env NODE_ENV=development nodemon --watch .env --watch . --inspect src/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint --fix",
      "yarn format",
      "git add"
    ],
    "*.{md,json}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-node": "9.1.0",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "nodemon": "1.19.1",
    "prettier": "1.18.2"
  },
  "dependencies": {
    "async-middleware": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "celebrate": "^10.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "es6-error": "^4.1.1",
    "express": "^4.16.4",
    "express-healthcheck": "^0.1.0",
    "helmet": "^3.16.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "lodash.once": "^4.1.1",
    "middleware-flow": "^0.8.0",
    "mongoose": "^5.5.1",
    "mongoose-seed": "^0.6.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.1.0",
    "nodemailer-mailgun-transport": "^1.4.0",
    "rotating-file-stream": "^1.4.0",
    "uuid": "^3.3.2"
  }
}
